{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "echo building!"
  },
  "deploy": {
    "startCommand": "gunicorn main:app -k uvicorn.workers.UvicornWorker --timeout 60",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100,
    "restartPolicyType": "ON_FAILURE"
  }
}
